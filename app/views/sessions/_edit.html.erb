<%= form_for(@event) do |f| %>
  <% if @event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% @event.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :titel %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :kommentar %><br>
    <%= f.text_area :comment %>
  </div>
  <div class="field">
    <%= f.label :datum %><br>
    <%= f.datetime_select :datetime %>
  </div>
  <div class="field">
    <%= f.label :roll %><br>
    <%= f.text_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :skola %><br>
    <%= f.text_field :school_id %>
  </div>
   <div class="field">
    <%= f.label :vecka %><br>
    <%= f.text_field :week %>
  </div>
  <div class="actions">
    <%= f.submit :class => "button tiny" %>
  </div>
<% end %>